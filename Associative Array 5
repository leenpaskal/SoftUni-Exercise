function solve(arr){

let assocArray = {};

let firstInput = arr.shift().split(" ");

for(let item of firstInput){
    assocArray[item] = 0;
}

for(let item of arr){
    if(item in assocArray){
        assocArray[item]++;
    }
}

let sortedArr = Object.entries(assocArray).sort((a,b) => b[1]-a[1]);

for(let [word, occurrences] of sortedArr){
    console.log(`${word} - ${occurrences}`);
};

}
